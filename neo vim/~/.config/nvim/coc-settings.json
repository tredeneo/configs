{
    "diagnostic.enable": true,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.messageTarget":"echo",
    "coc.preferences.formatOnSaveFiletypes":["python","dart"],
    "clangd.fallbackFlags": ["-Wall","-std=c99"],
    "clangd.disableSnippetCompletion": true,
    "snippets.snipmate.enable": false,
    "snippets.ultisnips.enable": true,
    "go.goplsPath": "~/.config/go/bin/gopls",
    "python.VenvPath":"~/.pyenv",
    "python.formatting.provider": "black",
    "python.jediEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": ["--ignore=W503,E203 --max-line-length 88"],
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": false,
    "python.analysis.diagnosticEnabled": true

}
